<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.daidonef.hobbiedata.Location" table="location">
	<id name="itemID" column="itemid" type="int">
		<generator class="increment"></generator>
	</id>
	<property name="accountID" column="accountid" type="int"></property>
	<property name="hobby" column="hobby" type="string"></property>
	<property name="genre" column="genre" type="string"></property>
	<property name="item" column="item" type="string"></property>
	<property name="location" column="location" type="string"></property>
	<property name="timeSpent" column="timespent" type="double"></property>
	<property name="dateStarted" column="datestarted" type="date"></property>
	<property name="lastDone" column="lastdone" type="date"></property>
	<property name="rating" column="rating" type="double"></property>
	<property name="description" column="description" type="string"></property>
	
</class>
</hibernate-mapping>